{
	"name": "np",
	"version": "7.6.4",
	"description": "A better `npm publish`",
	"license": "MIT",
	"repository": "sindresorhus/np",
	"funding": "https://github.com/sindresorhus/np?sponsor=1",
	"type": "module",
	"bin": "source/cli.js",
	"engines": {
		"node": ">=14.18",
		"npm": ">=6.8.0",
		"git": ">=2.11.0",
