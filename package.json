		"update-notifier": "^6.0.2"
	},
	"devDependencies": {
		"ava": "^5.2.0",
		"common-tags": "^1.8.2",
		"esmock": "^2.2.0",
		"sinon": "^15.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"files": [
			"!integration-test"
		],
		"nodeArguments": [
			"--loader=esmock"
		]
	},
	"xo": {
		"ignores": [
			"integration-test"
		],
		"rules": {
			"comma-dangle": "off",
			"object-shorthand": ["error", "properties"]
		}
	}
}
